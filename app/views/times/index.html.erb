<p id="notice"><%= notice %></p>

<h1>Listing Times</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @times.each do |time| %>
      <tr>
        <td><%= link_to 'Show', time %></td>
        <td><%= link_to 'Edit', edit_time_path(time) %></td>
        <td><%= link_to 'Destroy', time, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time', new_time_path %>
